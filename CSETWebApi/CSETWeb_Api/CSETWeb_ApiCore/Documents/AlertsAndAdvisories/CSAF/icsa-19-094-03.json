  "vulnerabilities": [
    {
      "cve": "CVE-2018-0466",
      "cwe": {
        "id": "CWE-399",
        "name": "Resource Management Errors"
      },
      "notes": [
        {
          "category": "summary",
          "text": "Sending a specially crafted open shortest path first version 3 (OSPFv3) packet could allow an unauthenticated, adjacent attacker to cause an affected device to reload, resulting in a denial-of-service condition.CVE-2018-0466 has been assigned to this vulnerability. A CVSS v3 base score of 7.4 has been calculated; the CVSS vector string is (AV:A/AC:L/PR:N/UI:N/S:C/C:N/I:N/A:H).",
          "title": "Vulnerability Summary"
        }
      ],

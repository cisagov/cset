<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CSETWebCore.Reports</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" />
    
</head>
<body>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CSETWebCore.Reports - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/jquery.min.js" ></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js" ></script>
    <script src="~/js/site.js" asp-append-version="true" asp-append-version="true"></script>
    <script src="~/js/chart.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

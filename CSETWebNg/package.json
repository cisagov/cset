        "@jsverse/transloco": "^7.5.1",
        "@kolkov/angular-editor": "^3.0.0-beta.2",
        "@ng-bootstrap/ng-bootstrap": "^18.0.0",
        "@ngstack/code-editor": "^9.0.0",
